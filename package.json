{
  "name": "intside-bootstrap-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:dev": "npx prisma migrate reset --force && npx prisma migrate dev --skip-generate && npx prisma generate && next build",
    "build:prod": "npx prisma generate --no-engine && npx prisma migrate deploy && next build",
    "build": "if [ \"$NODE_ENV\" = \"production\" ]; then yarn build:prod; else yarn build:dev; fi",
    "start": "next start",
    "lint": "next lint"
  },
  "prisma": {
    "seed": "ts-node --project tsconfig.local.json prisma/seeds/main.ts",
    "clear": "ts-node --project tsconfig.local.json prisma/seeds/clear.ts"
  },
  "overrides": {
    "swagger-ui-react": {
      "react": "^19.0.0",
      "react-dom": "^19.0.0"
    },
    "react-copy-to-clipboard": {
      "react": "^19.0.0"
    },
    "react-debounce-input": {
      "react": "^19.0.0"
    },
    "react-inspector": {
      "react": "^19.0.0"
    }
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.17",
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/interaction": "^6.1.17",
    "@fullcalendar/react": "^6.1.17",
    "@hookform/resolvers": "^5.0.1",
    "@popperjs/core": "^2.11.8",
    "@prisma/client": "^6.7.0",
    "axios": "^1.7.9",
    "bcrypt": "^6.0.0",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.16.0",
    "next": "15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.21.0",
    "yup": "^1.6.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/bcrypt": "^5.0.2",
    "@types/bootstrap": "^5.2.10",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-react": "^5.18.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "openapi-types": "^12.1.3",
    "prisma": "^6.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
